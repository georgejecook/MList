<?xml version='1.0' encoding='utf-8' ?>

<component
    name='CellProvider'
    extends='Group'>
  <script
      type='text/brightscript'
      uri='CellProvider.brs' />

  <interface>
    <field
        id='_debugM'
        type='boolean'
        onChange='onDebugMChange'
        alwaysNotify='true' />
    <field
        id='_m'
        type='assocarray' />
    <field
        id='isCreateMoreEnabled'
        type='boolean'
        value='false' />
    <field
        id='numberOfCellsInUse'
        type='integer'
        value='0' />
    <field
        id='numberOfCells'
        type='integer'
        value='0' />

    <function name='addPreBakedCells' />
    <function name='reset' />
    <function name='getCell' />
    <function name='releaseCell' />
  </interface>
</component>