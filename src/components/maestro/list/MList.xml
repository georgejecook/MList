<?xml version='1.0' encoding='utf-8' ?>
<component
    name='MList'
    extends='Group'>
  <interface>
    <field
        id='_debugM'
        type='boolean'
        onChange='onDebugMChange'
        alwaysNotify='true' />
    <field
        id='cellSize'
        type='floatArray'
        value='[300,300]' />
    <field
        id='cellSpacing'
        type='floatArray'
        value='[20,20]' />
    <!-- fixed|floating|focusWrap|zoom|custom -->
    <field
        id='rowFocusAnimationStyle'
        value='fixed'
        type='string' />
    <field
        id='numCols'
        type='integer'
        value='12' />
    <field
        id='focusIndexOffset'
        type='integer'
        value='0' />
    <field
        id='unfocusCellScale'
        type='float'
        value='0.5' />
    <field
        id='unfocusCount'
        type='float'
        value='3' />
    <field
        id='_m'
        type='assocarray' />
    <field
        id='content'
        type='node'
        onChange='onContentChange' />
    <field
        id='rowFocused'
        type='integer' />
    <field
        id='itemFocused'
        type='integer' />
    <field
        id='itemUnfocused'
        type='integer' />
    <field
        id='itemSelected'
        type='integer' />
    <field
        id='currFocusItemIndex'
        type='integer' />
    <field
        id='_focusedRenderer'
        type='node' />
    <function name='getBoundsForIndex' />
    <function name='getFocusedBounds' />

  </interface>
  <children>
    <Group id='rowsGroup' />
    <Rectangle
        id='overlay'
        color='#ff0000'
        opacity='0.5' />
  </children>

</component>