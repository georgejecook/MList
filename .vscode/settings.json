{
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "dist",
    "build",
  ],
  "files.exclude": {
    "build": true,
    ".history": true,
    "node_modules": true,
    "out": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "/tmp/**": true,
  },
  "search.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.meteor/local": true,
    "**/build/**": true,
    "build/**": true,
    "**/.history/**": true,
    ".history/**": true,
    ".history/**/*": true,
    "out/**": true,
    "**/out/**": true,
    "/tmp/**": true,
  },
  "files.useExperimentalFileWatcher": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.meteor/local": true,
    "**/build/**": true,
    "**/.history/**": true,
    ".history/**": true,
    "**/out/**": true,
  },
  "jira-plugin.workingProject": "",
  "workbench.colorCustomizations": {
    "statusBar.background": "#551A8B",
    "statusBar.debuggingBackground": "#551A8B",
    "panelTitle.activeBorder": "#ff0000",
    "activityBar.background": "#26226F",
    "titleBar.activeBackground": "#352F9B",
    "titleBar.activeForeground": "#FAFAFD"
  },

}