{
  "rootDir": "src",
  "files": [
    "manifest",
    "source/**/*.*",
    "components/**/*.*",
    "images/**/*.*",
    "!**/*.spec.bs",
    "!**/main.bs"
  ],
  "autoImportComponentScript": true,
  "createPackage": false,
  "stagingFolderPath": "dist",
  "diagnosticFilters": [
    {
      "src": "**/roku_modules/**/*.*",
      "codes": [
        1107,
        1009,
        1001,
        1013,
        1100,
        1004
      ]
    }
  ],
  "emitDefinitions": false
}